  <builders>
    <hudson.tasks.Shell>
      <command>git clone https://tescomobile.visualstudio.com/DefaultCollection/IGHS/_git/IGHS.iOS .

git submodule init

git submodule update --init --recursive</command>
    </hudson.tasks.Shell>
    <hudson.plugins.copyartifact.CopyArtifact plugin="copyartifact@1.31">
      <project>CI-Scripts</project>
      <filter>*.*</filter>
      <target></target>
      <excludes></excludes>
      <selector class="hudson.plugins.copyartifact.StatusBuildSelector"/>
      <doNotFingerprintArtifacts>false</doNotFingerprintArtifacts>
    </hudson.plugins.copyartifact.CopyArtifact>
    <hudson.tasks.Shell>
      <command>chmod -R 755 ${WORKSPACE}</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>security -v unlock-keychain -p ${keychain_password} &quot;/certs/login.keychain&quot;</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>/Applications/Xamarin?Studio.app/Contents/MacOS/mdtool -v build &quot;--configuration:AppStore|iPhone&quot; -p:Grocery.IGHS.Thailand.iOS Apps/Grocery/iOS/Grocery.IGHS/Grocery.IGHS.sln</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>mkdir -p ${WORKSPACE}/repo

mkdir ${WORKSPACE}/repo/${BUILD_NUMBER}/

ENT_BUILD=`find  ${WORKSPACE}/Apps/Grocery/iOS/Grocery.IGHS/Grocery.IGHS.Thailand.iOS/bin/iPhone/AppStore/  -name *.app -exec basename {} \; `

mv ${WORKSPACE}/Apps/Grocery/iOS/Grocery.IGHS/Grocery.IGHS.Thailand.iOS/bin/iPhone/AppStore/${ENT_BUILD} ${WORKSPACE}/repo/${BUILD_NUMBER}/${ENT_BUILD}

tar cf ${WORKSPACE}/repo/${ENT_BUILD}.${BUILD_NUMBER}.tar ${WORKSPACE}/repo/${BUILD_NUMBER}

cd ${WORKSPACE}/repo

bzip2 ${ENT_BUILD}.${BUILD_NUMBER}.tar</command>
    </hudson.tasks.Shell>
