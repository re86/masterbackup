  <builders>
    <hudson.plugins.copyartifact.CopyArtifact plugin="copyartifact@1.31">
      <project>StoreEmpower-iOS-Sanity_Build</project>
      <filter>**/*.ipa, **/*.bz2</filter>
      <target></target>
      <excludes></excludes>
      <selector class="hudson.plugins.copyartifact.StatusBuildSelector"/>
      <doNotFingerprintArtifacts>false</doNotFingerprintArtifacts>
    </hudson.plugins.copyartifact.CopyArtifact>
    <hudson.tasks.Shell>
      <command>tar jxf ${ARCHIVE_TO_COPY}</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>reports=$(date +&quot;%d%m%Y_%H_%M_%S&quot;)_Sanity.html
cd StoreEmpowerment-Automation
./runner.sh out/${START_BUILD_NUMBER}/StoreEmpower-iOS-${START_BUILD_NUMBER}.ipa
#cucumber features/login.feature features PLATFORM=iOS IOS_DEVICE=&quot;iPhone 6&quot; IOS_VERSION=8.1.2 IOS_UDID=98e71e5190ea23954cadb795900948ba60232f80 LOGIN=s2001Inform:TescoInform1A APP_PATH=out/${START_BUILD_NUMBER}/StoreEmpower-iOS-${START_BUILD_NUMBER}.ipa --format html -o $reports</command>
    </hudson.tasks.Shell>
  </builders>
  <publishers>
